<table class="common-table zebra-striped">
  <tr>
    <th>Name</th>
    <th>Date</th>
  </tr>
  <% @events.each do |event| %>
  <tr>
    <td><%= link_to event.name, event.url, :target => '_new', :onclick => "recordOutboundLink(this, '#{city_name}', '#{event.url}');return false;" %></td>
    <td><%= event.start_at %></td>
  </tr>
  <% end %>
</table>
